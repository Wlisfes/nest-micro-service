{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "apps/basic/src",
	"compilerOptions": {
		"deleteOutDir": true,
		"webpack": true,
		"tsConfigPath": "apps/basic/tsconfig.app.json"
	},
	"monorepo": true,
	"root": "apps/basic",
	"projects": {
		"basic": {
			"type": "application",
			"root": "apps/basic",
			"entryFile": "main",
			"sourceRoot": "apps/basic/src",
			"compilerOptions": {
				"tsConfigPath": "apps/basic/tsconfig.app.json"
			}
		},
		"mailer": {
			"type": "application",
			"root": "apps/mailer",
			"entryFile": "main",
			"sourceRoot": "apps/mailer/src",
			"compilerOptions": {
				"tsConfigPath": "apps/mailer/tsconfig.app.json"
			}
		},
		"configer": {
			"type": "library",
			"root": "libs/configer",
			"entryFile": "index",
			"sourceRoot": "libs/configer/src",
			"compilerOptions": {
				"tsConfigPath": "libs/configer/tsconfig.lib.json"
			}
		}
	}
}
